{
  "name": "@eduzz/houston-mobile",
  "description": "Houston mobile",
  "version": "0.31.0",
  "keywords": [
    "react-native",
    "houston-mobile",
    "ios",
    "android"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eduzz/houston-mobile.git",
    "directory": "packages/bundle"
  },
  "bugs": {
    "url": "https://github.com/eduzz/houston-mobile/issues"
  },
  "homepage": "https://github.com/eduzz/houston-mobile.git#readme",
  "main": "lib/commonjs/index.js",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@eduzz/houston-mobile-icons": "*",
    "@eduzz/houston-mobile-ui": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
